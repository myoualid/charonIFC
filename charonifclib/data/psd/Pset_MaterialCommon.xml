      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcNormalisedRatioMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
    <PropertyDef ifdguid="b82c30a2b54f43f489ae64b460fd2bfc">
      <Name>MassDensity</Name>
