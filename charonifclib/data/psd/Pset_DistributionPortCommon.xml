<?xml version="1.0"?>
<PropertySetDef xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd PSD_IFC4.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Pset_DistributionPortCommon</Name>
  <Definition>Common attributes attached to an instance of IfcDistributionPort.</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcDistributionPort</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcDistributionPort</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="f73610c88c2343ffa18cf690910ae409">
      <Name>PortNumber</Name>
      <Definition>The port index for logically ordering the port within the containing element or element type.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcInteger" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">ポート番号</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">ポートに含まれる要素、種類を示す番号。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="e27805e2da32494a8f6a258c69562d33">
      <Name>ColorCode</Name>
      <Definition>Name of a color for identifying the connector, if applicable.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">色番号</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">コネクタの色。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="ja-JP">IfcDistributionPortオブジェクトに関する基本属性。</PsetDefinitionAlias>
  </PsetDefinitionAliases>
</PropertySetDef>