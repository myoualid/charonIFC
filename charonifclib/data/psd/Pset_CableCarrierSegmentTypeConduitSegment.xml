<?xml version="1.0"?>
<PropertySetDef xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd PSD_IFC4.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Pset_CableCarrierSegmentTypeConduitSegment</Name>
  <Definition>An enclosed tubular carrier segment through which cables are pulled.
HISTORY: IFC2x4 - NominalLength deleted. To be handled as a quantity measure.</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcCableCarrierSegment/CONDUITSEGMENT</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcCableCarrierSegment/CONDUITSEGMENT</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="36bc1f3c8f0e4ef28a53d6d97acc7459">
      <Name>NominalWidth</Name>
      <Definition>The nominal width of the segment.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">公称幅</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">呼び幅寸法。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="d87a1b6cff9441b6bd7a469c5f7c0426">
      <Name>NominalHeight</Name>
      <Definition>The nominal height of the segment.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">公称高</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">呼び高寸法。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="4a8a4789fbca446b82104deaa98c0185">
      <Name>ConduitShapeType</Name>
      <Definition>The shape of the conduit segment.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_ConduitShapeType">
            <EnumItem>CIRCULAR</EnumItem>
            <EnumItem>OVAL</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">電線管のタイプ</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">電線管の形状タイプ。
（円筒形,卵形,その他,不明,なし）</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="cdfe43d97137438981184320e3340926">
      <Name>IsRigid</Name>
      <Definition>Indication of whether the conduit is rigid (= TRUE) or flexible (= FALSE).</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcBoolean" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">鋼管</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">鋼管か否か。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="ja-JP">電線管のプロパティを設定。</PsetDefinitionAlias>
  </PsetDefinitionAliases>
</PropertySetDef>