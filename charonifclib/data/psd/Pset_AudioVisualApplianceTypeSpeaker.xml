<?xml version="1.0"?>
<PropertySetDef xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd PSD_IFC4.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Pset_AudioVisualApplianceTypeSpeaker</Name>
  <Definition>An audio-visual speaker is a device that converts amplified audio signals into sound waves.  HISTORY: Added in IFC2x4.</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcAudioVisualAppliance/SPEAKER</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcAudioVisualAppliance/SPEAKER</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="286c64b5b06447b39c8097569e5f5523">
      <Name>SpeakerType</Name>
      <Definition>Indicates the type of speaker.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_AudioVisualSpeakerType">
            <EnumItem>FULLRANGE</EnumItem>
            <EnumItem>MIDRANGE</EnumItem>
            <EnumItem>WOOFER</EnumItem>
            <EnumItem>TWEETER</EnumItem>
            <EnumItem>COAXIAL</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET.</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">スピーカータイプ</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">スピーカーのタイプを示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="eaf4efd5106245088628dae6fa0d0135">
      <Name>SpeakerMounting</Name>
      <Definition>Indicates how the speaker is designed to be mounted.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_AudioVisualSpeakerMounting">
            <EnumItem>FREESTANDING</EnumItem>
            <EnumItem>CEILING</EnumItem>
            <EnumItem>WALL</EnumItem>
            <EnumItem>OUTDOOR</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET.</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">取付可能方法</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">取付可能な方法を示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="5998213a0efd4a86bdb4d1004685e0d3">
      <Name>SpeakerDriverSize</Name>
      <Definition>Indicates the number of drivers and their sizes.</Definition>
      <PropertyType>
        <TypePropertyTableValue />
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">スピーカードライバサイズ</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">ドライバユニットと数を示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="38776325266749049ffef9ab76c39db7">
      <Name>FrequencyResponse</Name>
      <Definition>Indicates the output over a specified range of frequencies.</Definition>
      <PropertyType>
        <TypePropertyTableValue />
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">周波数応答</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">周波数範囲での出力を示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="170c4c0cfc92451191763682f23465d4">
      <Name>Impedence</Name>
      <Definition>Indicates the speaker impedence.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcFrequencyMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">インピーダンス値</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">インピーダンス値を示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="ja-JP">音響信号を音波に変換する装置。</PsetDefinitionAlias>
  </PsetDefinitionAliases>
</PropertySetDef>