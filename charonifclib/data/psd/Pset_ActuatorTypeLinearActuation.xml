<?xml version="1.0"?>
<PropertySetDef xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd PSD_IFC4.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Pset_ActuatorTypeLinearActuation</Name>
  <Definition>Characteristics of linear actuation of an actuator
History: Replaces Pset_LinearActuator</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcActuator</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcActuator</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="df9701bfd9da4e43934ef055e724a8c5">
      <Name>Force</Name>
      <Definition>Indicates the maximum close-off force for the actuator.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcForceMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">最大推力</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">アクチュエータの最大締め切り力を示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="ef5450a04fc949b18a5b90dab1b94157">
      <Name>Stroke</Name>
      <Definition>Indicates the maximum distance the actuator must traverse.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">ストローク</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">アクチュエータが動く最大距離を示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="ja-JP">アクチュエータの直線動作の特性。</PsetDefinitionAlias>
  </PsetDefinitionAliases>
</PropertySetDef>