<?xml version="1.0"?>
<PropertySetDef xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd PSD_IFC4.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Pset_AudioVisualApplianceTypeTuner</Name>
  <Definition>An audio-visual tuner is a device that demodulates a signal into a stream of audio and/or video.  HISTORY: Added in IFC2x4.</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcAudioVisualAppliance/TUNER</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcAudioVisualAppliance/TUNER</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="4461f4d8010a406ea1617eb0e680ba0f">
      <Name>TunerType</Name>
      <Definition>Indicates the tuner type.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_AudioVisualTunerType">
            <EnumItem>AUDIO</EnumItem>
            <EnumItem>VIDEO</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET.</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">チューナータイプ</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">チューナータイプを示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="2c3ddd5402ae4356a26fb6d1170e9b9a">
      <Name>TunerMode</Name>
      <Definition>Indicates the tuner modes (or bands).  For example, 'AnalogCable', 'DigitalAir', 'AM', 'FM'.</Definition>
      <PropertyType>
        <TypePropertyTableValue />
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">チューナーモード</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">受信可能な放送モードを示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="ae592022b5614e70bdbd10e8248daaee">
      <Name>TunerChannel</Name>
      <Definition>Indicates the tuner channels, if applicable.</Definition>
      <PropertyType>
        <TypePropertyTableValue />
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">チューナーチャンネル</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">設定可能なチャンネルを示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="3f4aca7d8d0b4923b8aaf544d0e5769a">
      <Name>TunerFrequency</Name>
      <Definition>Indicates the tuner frequencies, if applicable.</Definition>
      <PropertyType>
        <TypePropertyBoundedValue>
          <DataType type="IfcFrequencyMeasure" />
        </TypePropertyBoundedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">周波数</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">対応周波数帯を示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="ja-JP">音響と映像の信号を変換する装置。</PsetDefinitionAlias>
  </PsetDefinitionAliases>
</PropertySetDef>