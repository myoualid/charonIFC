<?xml version="1.0"?>
<PropertySetDef xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd PSD_IFC4.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Pset_PipeSegmentTypeGutter</Name>
  <Definition>Gutter segment type common attributes.</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcPipeSegment/GUTTER</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcPipeSegment/GUTTER</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="d0e0d427a05a4ad188289a0109d61746">
      <Name>Slope</Name>
      <Definition>Angle of the gutter to allow for drainage.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPlaneAngleMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">勾配</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">排水に必要な溝の角度（傾斜）</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="53a3427e324b46feaefcfea4671a230c">
      <Name>FlowRating</Name>
      <Definition>Actual flow capacity for the gutter. Value of 0.00 means this value has not been set.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcVolumetricFlowRateMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">流量</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">実際の排水流量　　０.00値はこの値がｾｯﾄされていないことを意味する　</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases />
</PropertySetDef>