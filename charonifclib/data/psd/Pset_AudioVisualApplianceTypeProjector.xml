<?xml version="1.0"?>
<PropertySetDef xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd PSD_IFC4.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Pset_AudioVisualApplianceTypeProjector</Name>
  <Definition>An audio-visual projector is a device that projects video to a surface.  HISTORY: Added in IFC2x4.</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcAudioVisualAppliance/PROJECTOR</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcAudioVisualAppliance/PROJECTOR</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="9f28045407d34feda8059f996c694132">
      <Name>ProjectorType</Name>
      <Definition>Indicates the type of projector.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_AudioVisualProjectorType">
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET.</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">プロジェクター形式</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">プロジェクタの形式を示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="29908d3d98334da8957e6264d12ffbad">
      <Name>VideoResolutionWidth</Name>
      <Definition>Indicates the number of horizontal pixels (the largest native video resolution width).</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcInteger" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">水平解像度</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">水平方向のピクセル数を示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="c00ec43db7384efe8de2a88a790bf7fb">
      <Name>VideoResolutionHeight</Name>
      <Definition>Indicates the number of vertical pixels (the largest native video resolution height).</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcInteger" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">垂直解像度</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">垂直方向のピクセル数を示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="b2e4b75dc11843f3a85b990a4aa8661c">
      <Name>VideoResolutionMode</Name>
      <Definition>Indicates video resolution modes.</Definition>
      <PropertyType>
        <TypePropertyTableValue />
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">解像度モード</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">解像度モードを示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="62670a26d4fa46c8b904a0899c5049d4">
      <Name>VideoScaleMode</Name>
      <Definition>Indicates video scaling modes.</Definition>
      <PropertyType>
        <TypePropertyTableValue />
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">ビデオスケールモード</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">ビデオスケーリングモードを示す。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="1c086b7250bf4803b0283d1c79b3af0b">
      <Name>VideoCaptionMode</Name>
      <Definition>Indicates closed captioning modes.</Definition>
      <PropertyType>
        <TypePropertyTableValue />
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">ビデオキャプションモード</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">クローズドキャプションモードを示す。(字幕機能）</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="ja-JP">画面にビデオ映像を投影する装置。</PsetDefinitionAlias>
  </PsetDefinitionAliases>
</PropertySetDef>