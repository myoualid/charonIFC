  <Name>Pset_ValveTypeMixing</Name>
  <Definition>A valve where typically the temperature of the outlet is determined by mixing hot and cold water inlet flows.</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcValve/MIXING</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcValve/MIXING</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="d42b5e2ada9643c59f6977da33eeea20">
      <Name>MixerControl</Name>
      <Definition>Defines the form of control of the mixing valve.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_MixingValveControl">
            <EnumItem>MANUAL</EnumItem>
            <EnumItem>PREDEFINED</EnumItem>
            <EnumItem>THERMOSTATIC</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">混合制御</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">混合弁の制御形式の定義</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="bafec9cd03b642da976b736176678a99">
      <Name>OutletConnectionSize</Name>
      <Definition>The size of the pipework connection from the mixing valve.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">出力側接続口径</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">混合弁の配管接続サイズ</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="ja-JP">混合弁</PsetDefinitionAlias>
  </PsetDefinitionAliases>
</PropertySetDef>