        <TypePropertySingleValue>
          <DataType type="IfcInteger" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">区画数</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">管の区別される内部区画の個数。</DefinitionAlias>
      </DefinitionAliases>
