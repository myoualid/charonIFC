<?xml version="1.0"?>
<PropertySetDef xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd PSD_IFC4.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Pset_MaterialMechanical</Name>
  <Definition>A set of mechanical material properties normally used for structural analysis purpose. It contains all properties which are independent of the actual material type.</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcMaterial</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcMaterial</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="db04e64d02794391bc9a573bbc4dd9c8">
      <Name>DynamicViscosity</Name>
      <Definition>A measure of the viscous resistance of the material.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcDynamicViscosityMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
    <PropertyDef ifdguid="4063468b71c542f093a7c891405afd84">
      <Name>YoungModulus</Name>
      <Definition>A measure of the Young's modulus of elasticity of the material.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcModulusOfElasticityMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
    <PropertyDef ifdguid="22a9004538bb4309946dd609d064a31c">
      <Name>ShearModulus</Name>
      <Definition>A measure of the shear modulus of elasticity of the material.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcModulusOfElasticityMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
    <PropertyDef ifdguid="9daedfb4d0b2431ab67a1c14fcd1eee6">
      <Name>PoissonRatio</Name>
      <Definition>A measure of the lateral deformations in the elastic range.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveRatioMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
    <PropertyDef ifdguid="639a6102d8b14099a6b6b1310d4884a7">
      <Name>ThermalExpansionCoefficient</Name>
      <Definition>A measure of the expansion coefficient for warming up the material about one Kelvin.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcThermalExpansionCoefficientMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases />
</PropertySetDef>