<?xml version="1.0"?>
<PropertySetDef xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd PSD_IFC4.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Pset_HumidifierPHistory</Name>
  <Definition>Humidifier performance history attributes.
Sound attribute deleted in IFC2x2 Pset Addendum: Use IfcSoundProperties instead.</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcHumidifier</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcHumidifier</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="0052d384216b4b7fa7853f969cdca6bc">
      <Name>AtmosphericPressure</Name>
      <Definition>Ambient atmospheric pressure.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcPressureMeasure">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">標準大気圧（外気圧）</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">標準大気圧(外気圧）</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="fab041403c8e4e049690097526163078">
      <Name>SaturationEfficiency</Name>
      <Definition>Saturation efficiency: Ratio of leaving air absolute humidity to the maximum absolute humidity.</Definition>
      <PropertyType>
        <TypePropertyReferenceValue reftype="IfcNormalisedRatioMeasure">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">飽和比率</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">飽和比率：最大絶対湿度に対する現在の絶対湿度の割合</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases />
</PropertySetDef>