<?xml version="1.0"?>
<PropertySetDef xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd PSD_IFC4.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Pset_TubeBundleTypeFinned</Name>
  <Definition>Finned tube bundle type attributes.
Contains the attributes related to the fins attached to a tube in a finned tube bundle such as is commonly found in coils.</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcTubeBundle/FINNED</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcTubeBundle/FINNED</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="6cb121c0cc284089990da22b4081516d">
      <Name>Spacing</Name>
      <Definition>Distance between fins on a tube in the tube bundle.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">間隔</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">チューブ束内のフィン間の距離</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="62ef61826695452d8f39be14793e9e4c">
      <Name>Thickness</Name>
      <Definition>Thickness of the fin.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">厚さ</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">フィンの厚さ</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="4308f6cf511743f390201b74493e2a17">
      <Name>ThermalConductivity</Name>
      <Definition>The thermal conductivity of the fin.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcThermalConductivityMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">熱伝導率</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">フィンの熱伝導率</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="4942008c2657420bab0f6bc0352ca747">
      <Name>Length</Name>
      <Definition>Length of the fin as measured parallel to the direction of airflow.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">長さ</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">気流方向に平行に計られたフィンの長さ</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="3eccc585176f44d490bd3c37266efbe8">
      <Name>Height</Name>
      <Definition>Length of the fin as measured perpendicular to the direction of airflow.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">高さ</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">気流方向に垂直に計られたフィンの長さ</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="e28bfc9d42064ed69cbf94e9843a4dfe">
      <Name>Diameter</Name>
      <Definition>Actual diameter of a fin for circular fins only.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">直径</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">円形フィンのみ、フィンの実直径</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="b6319dde32b548aba53e9c91fa14b6c3">
      <Name>FinCorrugatedType</Name>
      <Definition>Description of a fin corrugated type.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcLabel" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">コルゲートタイプフィン</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">コルゲートタイプフィンの実直径</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="150efe5e4df64bf6aa7c880a379c0759">
      <Name>HasCoating</Name>
      <Definition>TRUE if the fin has a coating, FALSE if it does not.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcBoolean" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">コーティング有り</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">フィンがコーティング有りの場合 TRUE、無しの場合FALSE</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="ja-JP">フィンタイプチューブ束属性.一般にコイルに見られるように、フィンチューブ束内のチューブに付いているフィンに関する属性を含む</PsetDefinitionAlias>
  </PsetDefinitionAliases>
</PropertySetDef>