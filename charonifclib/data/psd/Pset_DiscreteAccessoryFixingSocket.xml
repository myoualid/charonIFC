<?xml version="1.0"?>
<PropertySetDef xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd PSD_IFC4.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Pset_DiscreteAccessoryFixingSocket</Name>
  <Definition>Properties common to fixing sockets.</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcDiscreteAccessory/Fixing socket</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcDiscreteAccessory/Fixing socket</ApplicableTypeValue>
  <PropertyDefs>
