        <TypePropertyReferenceValue reftype="">
          <DataType type="IfcExternalReference" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">参照基準</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">固定ソケットが参照するローカル標準。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="8ccd7cadce7343429299f7265cab7688">
