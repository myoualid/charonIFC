      <NameAliases>
        <NameAlias lang="ja-JP">換気回数</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">換気による熱負荷。</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="1ad1f339ccc649e0ae39f3f93138e572">
      <Name>DryBulbTemperature</Name>
      <Definition>Loads from the dry bulb temperature.</Definition>
