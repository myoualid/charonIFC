        <TypePropertyReferenceValue reftype="IfcNormalisedRatioMeasure">
          <DataType type="IfcTimeSeries" />
        </TypePropertyReferenceValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="ja-JP">機械効率</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="ja-JP">ポンプの稼動時の機械効率</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
