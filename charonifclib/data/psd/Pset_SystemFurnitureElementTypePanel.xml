<?xml version="1.0"?>
<PropertySetDef xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd PSD_IFC4.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>Pset_SystemFurnitureElementTypePanel</Name>
  <Definition>A set of specific properties for vertical panels that assembly workstations.. HISTORY: First issued in IFC Release R1.5. Renamed from Pset_Panel</Definition>
  <IfcVersion version="2x4" />
  <ApplicableClasses>
    <ClassName>IfcSystemFurnitureElement/PANEL</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcSystemFurnitureElement/PANEL</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="0cfa280e97da48e4bfe5077b45f0dc53">
      <Name>HasOpening</Name>
      <Definition>indicates whether the panel has an opening (= TRUE) or not (= FALSE).</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcBoolean" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
    <PropertyDef ifdguid="94726e7d5dbb453080f35be9e68bbfc1">
      <Name>FurniturePanelType</Name>
      <Definition>Available panel types from which that required may be selected.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_FurniturePanelType">
            <EnumItem>ACOUSTICAL</EnumItem>
            <EnumItem>GLAZED</EnumItem>
            <EnumItem>HORZ_SEG</EnumItem>
            <EnumItem>MONOLITHIC</EnumItem>
            <EnumItem>OPEN</EnumItem>
            <EnumItem>ENDS</EnumItem>
            <EnumItem>DOOR</EnumItem>
            <EnumItem>SCREEN</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
    <PropertyDef ifdguid="690621e035e04515add98ddaa938034d">
      <Name>NominalThickness</Name>
      <Definition>The nominal thickness of the panel.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases />
      <DefinitionAliases />
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases />
</PropertySetDef>